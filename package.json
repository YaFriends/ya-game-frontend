{
  "name": "ya-friends",
  "version": "1.0.4",
  "description": "Simple multi games app",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start:webpack": "webpack --mode=development --watch",
    "start:server": "nodemon index.js --watch dist/server.js",
    "start": "NODE_ENV=development PORT=8000 npm-run-all --print-label --parallel start:webpack start:server",
    "docker:start": "docker compose down && docker compose build && docker compose up -d --force-recreate",
    "build": "NODE_ENV=production && yarn clean && webpack --mode production",
    "docker:build": "docker compose down && docker compose build --build-arg DOCKER_ENV=production && docker compose up -d --force-recreate",
    "server": "node index.js",
    "test": "jest"
  },
  "keywords": [
    "yandex",
    "praktikum",
    "minigames"
  ],
  "author": "YaFriends Team",
  "license": "MIT",
